{
    "image": "${image_url}",
    "vcpus": ${etl_vcpus},
    "memory": ${etl_memory},
    "executionRoleArn": "${ecs_execution_role_arn}",
    "secrets": [

    ],
    "environment": [
        {
            "name": "ENVIRONMENT",
            "value": "${environment}"
        }
    ],
    "volumes": [
        {
            "host": {
                "sourcePath": "/media/ephemeral0"
            },
            "name": "ephemeral0"
        }
    ],
    "mountPoints": [
        {
            "containerPath": "/tmp",
            "readOnly": false,
            "sourceVolume": "ephemeral0"
        }
    ],
    "privileged": false
}
